// https://json5.dev
_x: 0,
git: 'github',
require: [
  'git',
  'nano',
  'xcodebuild'
],
cli: {
  install: {
    d: 'install ...',
    zsh: [
      'echo download http://127.0.0.1/sdk',
      'echo install -a'
    ]
  },
  start: {
    d: 'start ...',
    zsh: 'echo server -p #port -> 4000#'
  },
  build: {
    d: 'build ...',
    zsh: 'echo build -o #bin!#'
  },
  test: {
    d: 'test ...',
    zsh: 'echo test #suite#'
  }
}
