// https://json5.dev
_x: 0,
project: 'github',
require: [
  'git',
  'nano',
  'xcodebuild'
],
scripts: {
  install: {
    info: 'install ...',
    run: [
      'echo download http://127.0.0.1/sdk',
      'echo install -a'
    ]
  },
  start: {
    info: 'start ...',
    run: 'echo server -p #port -> 4000#'
  },
  build: {
    info: 'build ...',
    run: 'echo build -o #bin!#'
  },
  test: {
    info: 'test ...',
    run: 'echo test #suite#'
  }
}
